{
  "version": "1.0.9",
  "name": "@gg-santos/gittable",
  "description": "Modern Git CLI with Conventional Commits",
  "main": "index.js",
  "types": "index.d.ts",
  "bin": {
    "gittable": "index.js",
    "gg-gitz": "index.js"
  },
  "scripts": {
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "package": "npm publish --registry=https://npm.pkg.github.com && node scripts/publish-npm.js",
    "gittable": "node index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GG-Santos/Gittable.git"
  },
  "homepage": "https://github.com/GG-Santos/Gittable#readme",
  "bugs": {
    "url": "https://github.com/GG-Santos/Gittable/issues"
  },
  "author": "GG-Santos <ggsantos_0415@proton.me>",
  "license": "MIT",
  "keywords": [
    "git",
    "cli",
    "commit",
    "conventional-changelog",
    "git-wrapper",
    "version-control"
  ],
  "dependencies": {
    "@colors/colors": "^1.6.0",
    "chalk": "^4.1.2",
    "cli-cursor": "^4.0.0",
    "cli-spinners": "^3.3.0",
    "find-config": "^1.0.0",
    "is-interactive": "^1.0.0",
    "is-unicode-supported": "^1.3.0",
    "log-symbols": "^5.1.0",
    "prettycli": "^1.1.0",
    "stdin-discarder": "^0.3.0",
    "string-width": "^6.1.0",
    "strip-ansi": "^7.1.2",
    "terminal-link": "^3.0.0",
    "word-wrap": "^1.2.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4"
  },
  "overrides": {
    "glob": "^10.0.0"
  },
  "files": [
    "index.js",
    "src"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "type": "commonjs"
}
