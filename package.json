{
  "version": "1.0.7",
  "name": "@gg-santos/gittable",
  "description": "Modern Git CLI with Conventional Commits",
  "main": "index.js",
  "types": "index.d.ts",
  "bin": {
    "gittable": "index.js",
    "gg-gitz": "index.js"
  },
  "scripts": {
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "test": "node --test test/**/*.test.js",
    "test:watch": "node --test --watch test/**/*.test.js",
    "publish:github": "npm publish --registry=https://npm.pkg.github.com",
    "publish:npm": "node scripts/publish-npm.js",
    "publish:both": "npm run publish:github && npm run publish:npm",
    "gittable": "node index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GG-Santos/Gittable.git"
  },
  "homepage": "https://github.com/GG-Santos/Gittable#readme",
  "bugs": {
    "url": "https://github.com/GG-Santos/Gittable/issues"
  },
  "author": "GG-Santos <ggsantos_0415@proton.me>",
  "license": "MIT",
  "keywords": [
    "git",
    "cli",
    "commit",
    "conventional-changelog",
    "git-wrapper",
    "version-control"
  ],
  "dependencies": {
    "chalk": "^4.1.2",
    "cli-table3": "^0.6.5",
    "find-config": "^1.0.0",
    "prettycli": "^1.1.0",
    "sisteransi": "^1.0.5",
    "wcwidth": "^1.0.1",
    "word-wrap": "^1.2.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4"
  },
  "overrides": {
    "glob": "^10.0.0"
  },
  "files": [
    "index.js",
    "src"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "type": "commonjs"
}
